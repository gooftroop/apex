{
    "$schema": "https://json.schemastore.org/tsconfig",
    "compilerOptions": {
        "lib": ["esnext"],
        "skipLibCheck": true,
        "strict": true,
        "noEmit": true,
        "esModuleInterop": true,
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "incremental": true,
        "target": "ES2022",
        "paths": {
            "@/*": ["./*"]
        }
    },
    "exclude": ["node_modules"]
}
