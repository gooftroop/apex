{
    "author": "Brandon Webb (webb.c.brandon@gmail.com)",
    "dependencies": {
        "@apollo/client": "^3.10.4",
        "expo": "~51.0.12",
        "expo-status-bar": "~1.12.1",
        "graphql": "^16.8.1",
        "react": "18.2.0",
